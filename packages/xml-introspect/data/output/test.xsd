<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:dc="https://globalwordnet.github.io/schemas/dc/">
  <xs:element name="LexicalResource" type="LexicalResourceType"/>
  <xs:element name="GlobalInformation" type="GlobalInformationType"/>
  <xs:element name="label" type="labelType"/>
  <xs:element name="title" type="titleType"/>
  <xs:element name="description" type="descriptionType"/>
  <xs:element name="creator" type="creatorType"/>
  <xs:element name="contributor" type="contributorType"/>
  <xs:element name="date" type="dateType"/>
  <xs:element name="format" type="formatType"/>
  <xs:element name="language" type="languageType"/>
  <xs:element name="identifier" type="identifierType"/>
  <xs:element name="version" type="versionType"/>
  <xs:element name="license" type="licenseType"/>
  <xs:element name="Lexicon" type="LexiconType"/>
  <xs:element name="feat" type="featType"/>
  <xs:element name="LexicalEntry" type="LexicalEntryType"/>
  <xs:element name="Lemma" type="LemmaType"/>
  <xs:element name="Sense" type="SenseType"/>
  <xs:element name="Synset" type="SynsetType"/>
  <xs:element name="Definition" type="DefinitionType"/>
  <xs:element name="SynsetRelation" type="SynsetRelationType"/>

  <xs:complexType name="LexicalResourceType">
    <xs:sequence>
      <xs:element ref="GlobalInformation" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="Lexicon" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute ref="xmlns:dc"/>
  </xs:complexType>
  <xs:complexType name="GlobalInformationType">
    <xs:sequence>
      <xs:element ref="label" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="title" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="description" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="creator" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="contributor" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="date" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="format" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="language" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="identifier" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="version" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="license" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="labelType">
  </xs:complexType>
  <xs:complexType name="titleType">
  </xs:complexType>
  <xs:complexType name="descriptionType">
  </xs:complexType>
  <xs:complexType name="creatorType">
  </xs:complexType>
  <xs:complexType name="contributorType">
  </xs:complexType>
  <xs:complexType name="dateType">
  </xs:complexType>
  <xs:complexType name="formatType">
  </xs:complexType>
  <xs:complexType name="languageType">
  </xs:complexType>
  <xs:complexType name="identifierType">
  </xs:complexType>
  <xs:complexType name="versionType">
  </xs:complexType>
  <xs:complexType name="licenseType">
  </xs:complexType>
  <xs:complexType name="LexiconType">
    <xs:sequence>
      <xs:element ref="feat" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="LexicalEntry" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="Synset" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="id" type="xs:string"/>
    <xs:attribute name="label" type="xs:string"/>
    <xs:attribute name="language" type="xs:string"/>
    <xs:attribute name="email" type="xs:string"/>
    <xs:attribute name="license" type="xs:string"/>
    <xs:attribute name="version" type="xs:string"/>
    <xs:attribute name="url" type="xs:string"/>
  </xs:complexType>
  <xs:complexType name="featType">
    <xs:attribute name="att" type="xs:string"/>
    <xs:attribute name="val" type="xs:string"/>
  </xs:complexType>
  <xs:complexType name="LexicalEntryType">
    <xs:sequence>
      <xs:element ref="feat" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="Lemma" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="Sense" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="id" type="xs:string"/>
  </xs:complexType>
  <xs:complexType name="LemmaType">
    <xs:attribute name="writtenForm" type="xs:string"/>
    <xs:attribute name="partOfSpeech" type="xs:string"/>
  </xs:complexType>
  <xs:complexType name="SenseType">
    <xs:sequence>
      <xs:element ref="feat" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="id" type="xs:string"/>
    <xs:attribute name="synset" type="xs:string"/>
  </xs:complexType>
  <xs:complexType name="SynsetType">
    <xs:sequence>
      <xs:element ref="feat" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="Definition" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element ref="SynsetRelation" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="id" type="xs:string"/>
    <xs:attribute name="partOfSpeech" type="xs:string"/>
  </xs:complexType>
  <xs:complexType name="DefinitionType">
  </xs:complexType>
  <xs:complexType name="SynsetRelationType">
    <xs:attribute name="relType" type="xs:string"/>
    <xs:attribute name="target" type="xs:string"/>
  </xs:complexType>

  <xs:attribute name="dc:type" type="xs:string"/>
  <xs:attribute name="dc:source" type="xs:string"/>
  <xs:attribute name="dc:subject" type="xs:string"/>
</xs:schema>