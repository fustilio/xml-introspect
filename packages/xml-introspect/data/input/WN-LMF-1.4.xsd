<schema
  xmlns='http://www.w3.org/2000/10/XMLSchema'
  targetNamespace='http://www.w3.org/namespace/'
  xmlns:t='http://www.w3.org/namespace/'>

 <element name='LexicalResource'>
  <complexType>
   <choice maxOccurs='unbounded'>
    <element ref='t:Lexicon'/>
    <element ref='t:LexiconExtension'/>
   </choice>
   <attribute name='xmlns:dc' type='string' use='fixed' value='https://globalwordnet.github.io/schemas/dc/'/>
  </complexType>
 </element>

 <element name='Lexicon'>
  <complexType>
   <sequence>
    <element ref='t:Requires' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='t:LexicalEntry' maxOccurs='unbounded'/>
    <element ref='t:Synset' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='t:SyntacticBehaviour' minOccurs='0' maxOccurs='unbounded'/>
   </sequence>
   <attribute name='id' type='ID' use='required'/>
   <attribute name='label' type='string' use='required'/>
   <attribute name='language' type='string' use='required'/>
   <attribute name='email' type='string' use='required'/>
   <attribute name='license' type='string' use='required'/>
   <attribute name='version' type='string' use='required'/>
   <attribute name='url' type='string' use='optional'/>
   <attribute name='citation' type='string' use='optional'/>
   <attribute name='logo' type='string' use='optional'/>
   <attribute name='dc:contributor' type='string' use='optional'/>
   <attribute name='dc:coverage' type='string' use='optional'/>
   <attribute name='dc:creator' type='string' use='optional'/>
   <attribute name='dc:date' type='string' use='optional'/>
   <attribute name='dc:description' type='string' use='optional'/>
   <attribute name='dc:format' type='string' use='optional'/>
   <attribute name='dc:identifier' type='string' use='optional'/>
   <attribute name='dc:publisher' type='string' use='optional'/>
   <attribute name='dc:relation' type='string' use='optional'/>
   <attribute name='dc:rights' type='string' use='optional'/>
   <attribute name='dc:source' type='string' use='optional'/>
   <attribute name='dc:subject' type='string' use='optional'/>
   <attribute name='dc:title' type='string' use='optional'/>
   <attribute name='dc:type' type='string' use='optional'/>
   <attribute name='status' type='string' use='optional'/>
   <attribute name='note' type='string' use='optional'/>
   <attribute name='confidenceScore' type='string' use='default' value='1.0'/>
  </complexType>
 </element>

 <element name='LexicalEntry'>
  <complexType>
   <sequence>
    <element ref='t:Lemma'/>
    <element ref='t:Form' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='t:Sense' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='t:SyntacticBehaviour' minOccurs='0' maxOccurs='unbounded'/>
   </sequence>
   <attribute name='id' type='ID' use='required'/>
   <attribute name='dc:contributor' type='string' use='optional'/>
   <attribute name='dc:coverage' type='string' use='optional'/>
   <attribute name='dc:creator' type='string' use='optional'/>
   <attribute name='dc:date' type='string' use='optional'/>
   <attribute name='dc:description' type='string' use='optional'/>
   <attribute name='dc:format' type='string' use='optional'/>
   <attribute name='dc:identifier' type='string' use='optional'/>
   <attribute name='dc:publisher' type='string' use='optional'/>
   <attribute name='dc:relation' type='string' use='optional'/>
   <attribute name='dc:rights' type='string' use='optional'/>
   <attribute name='dc:source' type='string' use='optional'/>
   <attribute name='dc:subject' type='string' use='optional'/>
   <attribute name='dc:title' type='string' use='optional'/>
   <attribute name='dc:type' type='string' use='optional'/>
   <attribute name='status' type='string' use='optional'/>
   <attribute name='note' type='string' use='optional'/>
   <attribute name='index' type='string' use='optional'/>
   <attribute name='confidenceScore' type='string' use='optional'/>
  </complexType>
 </element>

 <element name='Lemma'>
  <complexType>
   <sequence>
    <element ref='t:Pronunciation' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='t:Tag' minOccurs='0' maxOccurs='unbounded'/>
   </sequence>
   <attribute name='writtenForm' type='string' use='required'/>
   <attribute name='script' type='string' use='optional'/>
   <attribute name='partOfSpeech' use='required'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='n'/>
      <enumeration value='v'/>
      <enumeration value='a'/>
      <enumeration value='r'/>
      <enumeration value='s'/>
      <enumeration value='t'/>
      <enumeration value='c'/>
      <enumeration value='p'/>
      <enumeration value='x'/>
      <enumeration value='u'/>
     </restriction>
    </simpleType>
   </attribute>
  </complexType>
 </element>

 <element name='Form'>
  <complexType>
   <sequence>
    <element ref='t:Pronunciation' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='t:Tag' minOccurs='0' maxOccurs='unbounded'/>
   </sequence>
   <attribute name='id' type='ID' use='optional'/>
   <attribute name='writtenForm' type='string' use='required'/>
   <attribute name='script' type='string' use='optional'/>
  </complexType>
 </element>

 <element name='Pronunciation'>
  <complexType mixed='true'>
   <attribute name='xml:space' use='default' value='default'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='default'/>
      <enumeration value='preserve'/>
     </restriction>
    </simpleType>
   </attribute>
   <attribute name='variety' type='string' use='optional'/>
   <attribute name='notation' type='string' use='optional'/>
   <attribute name='phonemic' use='default' value='true'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='true'/>
      <enumeration value='false'/>
     </restriction>
    </simpleType>
   </attribute>
   <attribute name='audio' type='string' use='optional'/>
  </complexType>
 </element>

 <element name='Tag'>
  <complexType mixed='true'>
   <attribute name='xml:space' use='default' value='default'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='default'/>
      <enumeration value='preserve'/>
     </restriction>
    </simpleType>
   </attribute>
   <attribute name='category' type='string' use='required'/>
  </complexType>
 </element>

 <element name='Sense'>
  <complexType>
   <sequence>
    <element ref='t:SenseRelation' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='t:Example' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='t:Count' minOccurs='0' maxOccurs='unbounded'/>
   </sequence>
   <attribute name='id' type='ID' use='required'/>
   <attribute name='synset' type='IDREF' use='required'/>
   <attribute name='n' type='string' use='optional'/>
