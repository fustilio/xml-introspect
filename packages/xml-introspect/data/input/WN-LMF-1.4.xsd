   <attribute name='confidenceScore' type='string' use='optional'/>
   <attribute name='lexicalized' use='default' value='true'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='true'/>
      <enumeration value='false'/>
     </restriction>
    </simpleType>
   </attribute>
   <attribute name='adjposition' use='optional'>
    <simpleType>
     <restriction base='string'>
      <enumeration value='a'/>
      <enumeration value='ip'/>
      <enumeration value='p'/>
     </restriction>
    </simpleType>
   </attribute>
   <attribute name='subcat' type='IDREFS' use='optional'/>
  </complexType>
 </element>

 <element name='Synset'>
  <complexType>
   <sequence>
